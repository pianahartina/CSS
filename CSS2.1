/* Pseudoclass 
CSS */

.hello:link {
	color: red;
}

.hello:hover {
	font-family: arial;
	font-size: 20px; 
}

.hello:active {
	font-style: italic;
}

.hello:visited{
	color: red;
}

li:first-child a{
	color: red;
}

li:last-child a{
	color: green;
}

li:nth-child(3) a{
	color: silver;
}

li:nth-child(odd) a:hover{
	color: green;
}

p:first-of-type {
	color: red; 
}
